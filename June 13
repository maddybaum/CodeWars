/**
 * @param {number[]} nums
 * @return {number}
 */
var removeDuplicates = function(nums) {
    let indexCounter = 0
    
    for(let i = 0; i < nums.length; i++){
        if (nums[i] != nums[i-1]){
            arr[indexCounter] = arr[i]
            indexCounter += 1  
        }
    }
    return indexCounter
};
